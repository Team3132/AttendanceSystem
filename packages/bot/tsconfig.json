{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"declaration": true,
		"removeComments": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"target": "ES2023",
		"sourceMap": true,
		"outDir": "./dist",
		"emitDeclarationOnly": true,
		"baseUrl": "./",
		"incremental": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	},
	"references": [
		{
			"path": "../frontend"
		}
	]
}
