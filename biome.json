{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"files": {
		"ignoreUnknown": false,
		"ignore": [
			"./src/routeTree.gen.ts",
			"./package.json",
			"./.output/**",
			"./.vinxi/**",
			"./dist/**",
			"./.tanstack-start/server-routes/routeTree.gen.ts",
			"src/gql/index.ts"
		]
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "tab"
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"correctness": {
				"noUnknownMediaFeatureName": "off",
				"noUnusedImports": "warn",
				"noUnusedVariables": "error"
			}
		}
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "double",
			"indentStyle": "space"
		},
		"parser": {
			"unsafeParameterDecoratorsEnabled": true
		}
	}
}
