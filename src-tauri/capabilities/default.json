{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "enables the default permissions",
	"windows": ["main"],
	"permissions": [
		"core:default",
		"deep-link:default",
		"http:default",
		"opener:default",
		{
			"identifier": "opener:allow-open-url",
			"allow": [
				{
					"url": "https://discord.com"
				}
			]
		},
		"store:default"
	]
}
